{
    "name": "jocoonopa/laravel-cross-database-subqueries",
    "description": "Eloquent cross database compatibility in subqueries",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/container": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/database": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/events": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^7.1",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Hoyvoy\\CrossDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hoyvoy\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hoyvoy\\CrossDatabase\\CrossDatabaseServiceProvider"
            ]
        }
    }
}
